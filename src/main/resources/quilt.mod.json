{
	"schema_version": 1,
	"quilt_loader": {
		"group": "xyz.fulmine",
		"id": "switchy_teleport",
		"version": "${version}",
		"metadata": {
			"name": "Switchy Teleport",
			"description": "Teleport to your last location whenever you use Switchy.",
			"contributors": {
				"Fulmine": "Owner"
			},
			"contact": {
				"issues": "https://github.com/draconizations/switchy-teleport/issues",
